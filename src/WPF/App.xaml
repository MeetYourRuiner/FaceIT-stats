<Application x:Class="FaceitStats.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FaceitStats.WPF.Views"
             Startup="App_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Views/Styles/Colors.xaml"/>
                <ResourceDictionary Source="/Views/Styles/Controls.xaml"/>
                <ResourceDictionary Source="/Views/Styles/DataTemplates.xaml"/>
                <ResourceDictionary Source="/Views/Styles/Converters.xaml"/>
                <ResourceDictionary Source="/Views/Styles/DynamicResources.xaml"/>
                <ResourceDictionary Source="/Views/Styles/Images.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
